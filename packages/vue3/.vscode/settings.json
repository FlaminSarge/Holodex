{
  "i18n-ally.localesPaths": [
    "src/locales"
  ],
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "i18n-ally.dirStructure": "dir",
  "i18n-ally.enabledParsers": [
    "yaml"
  ],
  "i18n-ally.pathMatcher": "{locale}/ui.yml",
  "i18n-ally.keystyle": "nested",
  "scss.lint.unknownAtRules": "ignore",
  "css.lint.unknownAtRules": "ignore",
  "typescript.tsdk": "node_modules/typescript/lib",
  "vue3snippets.enable-compile-vue-file-on-did-save-code": true,
  "eslint.format.enable": true,
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "vue.codeActions.enabled": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#1F1A1D",
    "activityBar.foreground": "#C2AAB7",
    "activityBar.inactiveForeground": "#82767D",
    "activityBar.activeBorder": "#D38FA8",
    "activityBar.activeBackground": "#312D31",
    "activityBar.border": "#312D31",
    "titleBar.activeBackground": "#1F1A1D",
    "titleBar.activeForeground": "#C2AAB7",
    "titleBar.inactiveBackground": "#2A2528",
    "titleBar.inactiveForeground": "#82767D",
    "titleBar.border": "#312D31",
    "statusBar.background": "#1F1A1D",
    "statusBar.foreground": "#C2AAB7",
    "statusBar.border": "#312D31"
  }
}